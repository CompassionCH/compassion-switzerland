<odoo>
    <record id="view_correspondance_template_website" model="ir.ui.view">
        <field name="name">view.correspondance.template.website</field>
        <field name="model">correspondence.template</field>
        <field name="priority" eval="100"/>
        <field name="inherit_id" ref="sbc_compassion.view_correspondence_template_origin_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='active']" position="after">
                <field name="website_published"/>
            </xpath>
        </field>
    </record>
</odoo>
