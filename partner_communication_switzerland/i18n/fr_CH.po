#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_hold_removal
msgid ""
"<div>\n"
"                    % set hold = object.get_objects()\n"
"                    % set owner = object.partner_id.firstname\n"
"                    % set child_code = hold.mapped('child_id')."
"get_list('local_id')\n"
"                    % set child_name = hold.mapped('child_id')."
"get_list('name')\n"
"                </div>\n"
"                <p>\n"
"                    Dear ${owner},\n"
"                    <br>\n"
"                    <br>\n"
"                    We just learned that the child ${child_name} with ref. "
"${child_code} is no longer available for sponsorship.\n"
"                    The following comments were received :\n"
"                    % for h in hold:\n"
"                        <br>\n"
"                        ${h.comments}\n"
"                        <br>\n"
"                    % endfor\n"
"                    <br>\n"
"                    <br>\n"
"                    If you have the childpack, please don't use it.\n"
"                    <br>\n"
"                    <br>\n"
"                    Yours sincerely\n"
"                </p>\n"
"            "
msgstr ""

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.event_letter_template
msgid ""
"<div>\n"
"                    % set invoice_lines = object.get_objects()\n"
"                    % set donations = invoice_lines.get_donations()\n"
"                    % set partner = object.partner_id\n"
"                    % set ambassadors = invoice_lines.mapped('user_id')\n"
"                    % set count = 0\n"
"                    % set currency = invoice_lines[0].invoice_id.currency_id."
"name\n"
"                </div>\n"
"                <div>\n"
"                    ${partner.salutation},\n"
"                    <br>\n"
"                    <br>\n"
"                    On behalf of Compassion Switzerland we truly thank you "
"for your ${'generous' if object.config_id.id != 52 else ''} gift of <br>"
"${currency} ${donations[0]}.-\n"
"                    % if donations[1]:\n"
"                        <!-- space --> supporting\n"
"                        % if ambassadors:\n"
"                            <!-- space --> ${ambassadors."
"get_list('full_name')}\n"
"                        % endif\n"
"                            <!-- space --> for the ${donations[1]}.\n"
"                    % endif\n"
"                    <br>\n"
"                    <br>\n"
"                    % for amb in ambassadors.filtered('advocate_details_id."
"quote'):\n"
"                       % set count = count + 1\n"
"<div id=\"ambassador_quote\">\n"
"                        ${amb.advocate_details_id.thank_you_quote |safe}\n"
"<br>\n"
"</div>\n"
"                         % if len(ambassadors) != 1 and len(ambassadors) != "
"count:\n"
"                                  <br>\n"
"                                  <br>\n"
"                                  <br>\n"
"                           % endif\n"
"                    % endfor\n"
"% if len(ambassadors) == 2:\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"% endif\n"
"                    % if object.event_id.thank_you_text:\n"
"                        ${object.event_id.thank_you_text |safe}\n"
"                    % endif\n"
"                    % if partner.number_sponsorships:\n"
"                        Have you recently written to your sponsor child? Our "
"website allows you to write a letter directly online. Why not have another "
"go at writing this week?\n"
"                    % else:\n"
"                        % if partner.is_new_donor:\n"
"                            Since 1952, Compassion has been pursuing a very "
"specific mission: to release as many children as possible from extreme "
"poverty – in Jesus’ name.\n"
"In collaboration with 8000 local churches, we currently support 2 million "
"children in 25 countries of the global south.\n"
"                            <br>\n"
"                            <br>\n"
"                        % endif\n"
"                        % if not partner.opt_out:\n"
"                            Is the future of the world's poorest children "
"important to you? Compassion offers different ways and solutions to release "
"children from poverty. At the heart of Compassion’s ministry is the Child "
"Sponsorship program. Maybe you would consider sponsoring a child and "
"offering support and encouragement throughout their development? You can "
"find further information on\n"
"www.compassion.ch or you can call us on 0800 874 773.\n"
"                                 <br>\n"
"                                 <br>\n"
"                        % endif\n"
"                    % endif\n"
"                    We just want to take this opportunity to thank you once "
"again for your generosity and support towards these children in need.\n"
"                    <br>\n"
"                    <br>\n"
"                    Sincerely yours\n"
"% if object.send_mode == 'digital':\n"
"   <br>\n"
"   <br>\n"
"    Rachel Maglo\n"
"    <br>\n"
"    Donors relation\n"
"    <br>\n"
"    Compassion Switzerland\n"
"% endif\n"
"% if len(ambassadors) != 0 and len(ambassadors) != 1:\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"% endif\n"
"                </div>\n"
"            "
msgstr ""
"<div>\n"
"                    % set invoice_lines = object.get_objects()\n"
"                    % set donations = invoice_lines.get_donations()\n"
"                    % set partner = object.partner_id\n"
"                    % set ambassadors = invoice_lines.mapped('user_id')\n"
"                    % set count = 0\n"
"                    % set currency = invoice_lines[0].invoice_id.currency_id."
"name\n"
"</div>\n"
"                <div>\n"
"                    ${partner.salutation},\n"
"                    <br>\n"
"                    <br>\n"
"                    Compassion Suisse vous remercie infiniment pour votre "
"${'généreux' if object.config_id.id != 52 else ''} don de <br>${currency} "
"${donations[0]}.- \n"
"                    % if donations[1]:\n"
"                        % if ambassadors:\n"
"                            <!-- space --> pour ${ambassadors."
"get_list('full_name')} \n"
"                        % endif\n"
"                            <!-- space --> (${donations[1]}).\n"
"                    % else\n"
"                        .\n"
"                    % endif\n"
"                    <br>\n"
"                    <br>\n"
"                    % for amb in ambassadors.filtered('advocate_details_id."
"quote'):\n"
"                       % set count = count + 1\n"
"<div id=\"ambassador_quote\">\n"
