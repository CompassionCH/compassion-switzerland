"\n"
"    % if moms > 0 and moms < 1:\n"
"        % set moms = 1\n"
"    % endif\n"
"    % if toilets > 0 and toilets < 1:\n"
"        % set toilets = 1\n"
"    % endif\n"
"    % if christmas > 0 and christmas < 1:\n"
"        % set christmas = 1\n"
"    % endif\n"
