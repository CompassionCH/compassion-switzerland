msgid "<p>\n"
"                    % set partner = object.partner_id\n"
"                    % set is_plural = partner.title.plural or partner.title.id == 29\n"
"                    % set dir = 'euch' if is_plural else 'dir'\n"
"                    % set du = 'ihr' if is_plural else 'du'\n"
