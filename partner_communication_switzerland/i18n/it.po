"  <br>\n"
"  <br>\n"
"  Sincerely\n"
" "
msgstr ""
"<p>\n"
"  </p>\n"
"  <div>\n"
"   % set contracts = object.get_objects()\n"
"                    \n"
"% set partner = object.partner_id\n"
"% set christmas_fund_2022 = 265 in partner.category_id.ids\n"
"\n"
