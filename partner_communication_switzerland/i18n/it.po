#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.event_letter_template
msgid "<div>\n"
"                    % set invoice_lines = object.get_objects()\n"
"                    % set donations = invoice_lines.get_donations()\n"
"                    % set partner = object.partner_id\n"
"                    % set ambassadors = invoice_lines.mapped('user_id')\n"
"                    % set count = 0\n"
"                    % set currency = invoice_lines[0].invoice_id.currency_id.name\n"
"                </div>\n"
"                <div>\n"
"                    ${partner.salutation},\n"
"                    <br>\n"
"                    <br>\n"
"                    On behalf of Compassion Switzerland we truly thank you for your ${'generous' if object.config_id.id != 52 else ''} gift of <br>${currency} ${donations[0]}.-\n"
"                    % if donations[1]:\n"
"                        <!-- space --> supporting\n"
"                        % if ambassadors:\n"
"                            <!-- space --> ${ambassadors.get_list('full_name')}\n"
"                        % endif\n"
"                            <!-- space --> for the ${donations[1]}.\n"
"                    % endif\n"
"                    <br>\n"
"                    <br>\n"
"                    % for amb in ambassadors.filtered('advocate_details_id.quote'):\n"
"                       % set count = count + 1\n"
"<div id=\"ambassador_quote\">\n"
"                        ${amb.advocate_details_id.thank_you_quote |safe}\n"
"<br>\n"
"</div>\n"
"                         % if len(ambassadors) != 1 and len(ambassadors) != count:\n"
"                                  <br>\n"
"                                  <br>\n"
"                                  <br>\n"
"                           % endif\n"
"                    % endfor\n"
"% if len(ambassadors) == 2:\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"% endif\n"
"                    % if object.event_id.thank_you_text:\n"
"                        ${object.event_id.thank_you_text |safe}\n"
"                    % endif\n"
"                    % if partner.number_sponsorships:\n"
"                        Have you recently written to your sponsor child? Our website allows you to write a letter directly online. Why not have another go at writing this week?\n"
"                    % else:\n"
"                        % if partner.is_new_donor:\n"
"                            Since 1952, Compassion has been pursuing a very specific mission: to release as many children as possible from extreme poverty – in Jesus’ name.\n"
"In collaboration with 8000 local churches, we currently support 2 million children in 25 countries of the global south.\n"
"                            <br>\n"
"                            <br>\n"
"                        % endif\n"
"                        % if not partner.opt_out:\n"
"                            Is the future of the world's poorest children important to you? Compassion offers different ways and solutions to release children from poverty. At the heart of Compassion’s ministry is the Child Sponsorship program. Maybe you would consider sponsoring a child and offering support and encouragement throughout their development? You can find further information on\n"
"www.compassion.ch or you can call us on 0800 874 773.\n"
"                                 <br>\n"
"                                 <br>\n"
"                        % endif\n"
"                    % endif\n"
"                    We just want to take this opportunity to thank you once again for your generosity and support towards these children in need.\n"
"                    <br>\n"
"                    <br>\n"
"                    Sincerely yours\n"
"% if object.send_mode == 'digital':\n"
"   <br>\n"
"   <br>\n"
"    Rachel Maglo\n"
"    <br>\n"
"    Donors relation\n"
"    <br>\n"
"    Compassion Switzerland\n"
"% endif\n"
"% if len(ambassadors) != 0 and len(ambassadors) != 1:\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"% endif\n"
"                </div>\n"
"            "
msgstr "<div>\n"
"                    % set invoice_lines = object.get_objects()\n"
"                    % set donations = invoice_lines.get_donations()\n"
"                    % set partner = object.partner_id\n"
"                    % set ambassadors = invoice_lines.mapped('user_id')\n"
"                    % set count = 0\n"
"                    % set currency = invoice_lines[0].invoice_id.currency_id.name\n"
"                </div>\n"
"                <div>\n"
"                    ${partner.salutation},\n"
"                    <br>\n"
"                    <br>\n"
"                    Compassion Svizzera vi ringrazia per la vostra ${'generosa' if object.config_id.id != 52 else ''} donazione del valore di <br>${currency} ${donations[0]}.-\n"
"                    % if donations[1]:\n"
"                        % if ambassadors:\n"
"                            <!-- space --> per ${ambassadors.get_list('full_name')}\n"
"                        % endif\n"
"                            <!-- space --> (${donations[1]}).\n"
"                    % else\n"
"                        .\n"
"                    % endif\n"
"                    <br>\n"
"                    <br>\n"
"                    % for amb in ambassadors.filtered('advocate_details_id.quote'):\n"
"                       % set count = count + 1\n"
"<div id=\"ambassador_quote\">\n"
"                        ${amb.advocate_details_id.thank_you_quote |safe}\n"
"<br>\n"
"</div>\n"
"                         % if len(ambassadors) != 1 and len(ambassadors) != count:\n"
"                                  <br>\n"
"                                  <br>\n"
"                                  <br>\n"
"                           % endif\n"
"                    % endfor\n"
"% if len(ambassadors) == 2:\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"% endif\n"
"                    % if object.event_id.thank_you_text:\n"
"                        ${object.event_id.thank_you_text |safe}\n"
"                    % endif\n"
"                    % if partner.number_sponsorships:\n"
"                        Avete scritto recentemente al vostro bambino ? Il nostro sito internet vi permette di scrivere una lettera direttamente on-line. Perchè non farlo questa settimana ?\n"
"                    % else:\n"
"                        % if partner.is_new_donor:\n"
"                            Dal 1952, Compassion è impegnata in una missione ben specifica: liberare più bambini possibili dall’estrema povertà. In collaborazione con 8000 Chiese locali, siamo in grado di sostenere e partecipare allo sviluppo di 2 milioni di bambini nei 25 Paesi in cui operiamo.\n"
"                            <br>\n"
"                            <br>\n"
"                        % endif\n"
"                            Avete a cuore il destino dei bambini tra i più poveri del mondo? Compassion vi offre varie possibilità per impeganrvi nel liberare i bambini dalla povertà. Tra queste, potete sostenere un bambino, espimergli il vostro interesse, incoraggiarlo nella sua crescita. \n"
"Trovate più informazioni sul sito  www.compassion.ch o contattandoci al numero 0800 874 773.\n"
"                    % endif\n"
"                    <br>\n"
"                    <br>\n"
"                    Ancora una volta, grazie infinite per la vostra generosità a favore dei bambini bisognosi.\n"
"                    <br>\n"
"                    <br>\n"
"                    Con i nostri migliori saluti.\n"
"% if object.send_mode == 'digital':\n"
"   <br>\n"
"   <br>\n"
"    Rachel Maglo\n"
"    <br>\n"
"    Relazioni donatori\n"
"    <br>\n"
"    Compassion Svizzera\n"
"% endif\n"
"% if len(ambassadors) != 0 and len(ambassadors) != 1:\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"                    <br>\n"
"% endif\n"
"                </div>\n"
"            "

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_biennial
msgid "<div>\n"
"                    % set partner = object.partner_id\n"
"                    % set child = object.get_objects()\n"
"                    % set firstname = child.get_list('preferred_name', 3, 'many children', False)\n"
"                </div>\n"
"                <p>\n"
"                    ${partner.salutation},\n"
"                    <br>\n"
"                    <br>\n"
"                    Thank you from the bottom of our hearts for your support to ${firstname}. Compassion Switzerland is pleased to send you a new photo of ${child.get('your sponsored child')} so you will see how ${child.get('he') + ' ' + child.get('has')} grown up.\n"
"                    % if object.send_mode == 'both':\n"
"                        <br>\n"
"                        <br>\n"
"                        We will send you the printed picture as we used to do in the following weeks. In case you don't need it, or you want to stop receiving pictures by e-mail, please let us know by answering to this e-mail.\n"
"                    % endif\n"
"                    <br>\n"
"                    <br>\n"
"                    Thanks to your generous and faithful support, the employees of the centre can offer a framework, accompaniment and activities to ${child.get('your sponsored child')} which allow ${child.get('him')} to develop and flourish in ${child.get('him')}self, in ${child.get('his')} health; in personal responsibilities and spiritualy. Through your sponsorship, you also allow ${child.get('this child')} to have a privileged relationship with a person living on the other side of the world.\n"
"<br>\n"
"<br>\n"
"        The exchange of letters is a key element of child sponsorship. Thank you for your commitment alongside Compassion to free children from poverty in the name of Jesus.\n"
"                    <br>\n"
"                    <br>\n"
"                    Yours sincerely\n"
"% if object.send_mode != 'physical':\n"
"   <br>\n"
"   <br>\n"
"    Carole Rochat\n"
"    <br>\n"
"    Sponsor and Donor services\n"
"    <br>\n"
"    Compassion Switzerland\n"
"% endif\n"
"                </p>\n"
"            "
msgstr "<div>\n"
"    % set partner = object.partner_id\n"
"    % set child = object.get_objects()\n"
"    % set firstname = child.get_list('preferred_name', False)\n"
"</div>\n"
"<p>\n"
"    ${partner.salutation},\n"
"    <br>\n"
"    <br>\n"
"    Grazie di cuore per il vostro sostegno a ${firstname}. Compassion Svizzera è felice di mandarvi una foto del vostro bambino. Potete così vedere come ${child.get('is')} cresciut${child.get('accord_o_a')}.\n"
"% if object.send_mode == 'both':\n"
"    <br>\n"
"    <br>\n"
"    Vi invieremo la foto per posta nelle prossime settimane, come fatto in precedenza. Se desiderate cambiare il modo in cui ricevere le foto dei bambini sostenuti (solo per posta o e-mail), vi preghiamo di comunicarcelo rispondendo a questa e-mail.\n"
"    % endif\n"
"    <br>\n"
"    <br>\n"
"    Grazie al vostro generoso e fedele sostegno, i collaboratori del centro potranno offrire un accompagnamento e delle attività a ${firstname}, in un ambiente protetto, che gli permetterà di crescere sotto il profilo personale,  spirituale e responsabile con attenzione alla sanità. Il vostro sostegno, inoltre, permette al bambino di vivere una relazione privilegiata con una persona che vive dall'altra parte del mondo.\n"
"<br>\n"
"<br>\n"
"${object.success_sentence or ''}\n"
"<br>\n"
"<br>\n"
"    Con i nostri migliori saluti.\n"
"% if object.send_mode != 'physical':\n"
"   <br>\n"
"   <br>\n"
"    Laura Tedino\n"
"    <br>\n"
"    Dipartimento di Sostgegno\n"
"    <br>\n"
"    Compassion Svizzera\n"
"% endif\n"
"</p>"

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_sponsorship_reminder_2
msgid "<div>\n"
"                    % set partner = object.partner_id\n"
"                    % set sponsorships = object.get_objects()\n"
"                    % set children = sponsorships.mapped('child_id')\n"
"                    % set firstname = children.get_list('preferred_name', 3, children.get_number(), False)\n"
"                    % set invoices = sponsorships.mapped('due_invoice_ids')\n"
"                    % set amount = sum(invoices.mapped('amount_total'))\n"
"                    % set number = max(sponsorships.mapped('months_due'))\n"
"                    % set month = invoices.get_date('date_invoice', 'MMMM')\n"
"   % set letter = object.send_mode == 'physical'\n"
"    % set payment_term = sponsorships.mapped('payment_mode_id.name')[0]\n"
"    % set lsv_dd = 'LSV' in payment_term or 'Postfinance' in payment_term\n"
"                </div>\n"
"                <p>\n"
"                    ${partner.salutation},\n"
"                    <br>\n"
"                    <br>\n"
"                    We very warmly thank you for your support toward ${firstname}.\n"
"                    <br>\n"
"                    <br>\n"
"                   Despite our previous reminder, <!-- Keep space -->\n"
"                   % if lsv_dd:\n"
"                        we notice that we were not able to debit your account ${number} times in ${month}, which represents the amount of <b>CHF ${'%d' | format(amount)}.-</b>\n"
"                    <br>\n"
"                    <br>\n"
"                        Therefore, we kindly ask you to use the payment slip attached to transfer the amount <b>within 10 days</b>. Otherwise we will be forced to end your sponsorship and look for another sponsor for ${firstname}.\n"
"                        <br>\n"
"                        Please contact us if you are not able to pay at the moment. Together we will look for a solution which guarantees an ongoing support for your sponsored child. \n"
"                        <br>\n"
"                        <br>\n"
"                        For the future, we suggest you to change your mean of payment and to use the reference of the payment slip to settle a monthly permanent order. You may also communicate us a new account number, fill the authorization form <!-- Keep space -->\n"
"                        % if object.send_mode != 'physical':    \n"
"                            (a href=\"https://compassion.ch/wp-content/uploads/documents_compassion/Formulaire_LSV_DD_FR.pdf\" style=\"color: skyblue; text-decoration: underline;\"&gt;download here) <!-- Keep space -->\n"
"                        % endif\n"
"and send it back to our office.\n"
"                   % else:\n"
"                   we notice that your sponsorship has not been paid ${number} times in ${month}, which represents <b>the amount of CHF ${'%d' | format(amount)}.-</b>\n"
"                    <br>\n"
"                    <br>\n"
"                    We kindly ask you to use the payment slip attached to transfer the amount <b>within 10 days</b>. Otherwise we will be forced to end your sponsorship and look for another sponsor for ${firstname}.\n"
"                    <br>\n"
"                    Please contact us if you are not able to pay at the moment. Together we will look for a solution which guarantees an ongoing support for your sponsored child.\n"
"                    <br>\n"
"                    <br>\n"
"                    If this letter crosses with your payment, please accept our thanks and ignore this notice. Thanks for making the payment of your sponsorship a priority.\n"
"                    <br>\n"
"                    <br>\n"
"                    Sponsoring a child is one of the most promising investments to free a child from extreme poverty\n"
"                   % endif\n"
"                    <br>\n"
"                    <br>\n"
"                    Sincerely,\n"
"                </p>\n"
"            "
msgstr "<div>\n"
"    % set partner = object.partner_id\n"
"    % set sponsorships = object.get_objects()\n"
"    % set children = sponsorships.mapped('child_id')\n"
"    % set firstname = children.get_list('preferred_name', 3, children.get_number(), False)\n"
"    % set invoices = sponsorships.mapped('due_invoice_ids')\n"
"    % set amount = sum(invoices.mapped('amount_total'))\n"
"    % set number = max(sponsorships.mapped('months_due'))\n"
"    % set month = invoices.get_date('date_invoice', 'MMMM')\n"
"</div>\n"
"<p>\n"
"    ${partner.salutation},\n"
"    <br>\n"
"    <br>\n"
"    Vi ringraziamo calorosamente per il vostro impegno in favore di ${firstname}.\n"
"    <br>\n"
"    <br>\n"
"Abbiamo notato che il nostro richiamo di pagamento per il vostro sostengo non è stato regolato per ${number} mesi (${month}), per un totale di CHF ${'%d' | format(amount)}.-\n"
"    <br>\n"
"    <br>\n"
"    <b>Vi preghiamo di regolarizzare il pagamento entro 10 giorni</b> utilizzando il bollettino di versamento allegato. Non esitate a contattarci se non siete in grado di effettuare il pagamento ora.\n"
"    <br>\n"
"    <br>\n"
"    Vi preghiamo di contattarci se desiderate effettuare un pagamento scaglionato. Insieme troveremo una soluzione per garantire al vostro bambino supporto continuo. In caso contrario, provvederemo ad annullare il sostegno e a cercare un nuovo sostenitore per ${firstname}.\n"
"    <br>\n"
"    <br>\n"
"    Se nel frattempo avete già eseguito il pagamento, vi preghiamo di non considerare la presente comunicazione.\n"
"    <br>\n"
"    <br>\n"
"Il sostegno a distanza è uno degli investimenti più promettenti per liberare un bambino dall'estrema povertà.\n"
"    <br>\n"
"    <br>\n"
"    Fraterni saluti.\n"
"</p>"

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.sms_confirmation_email_1
msgid "<div>\n"
"                    % set sponsorship = object.get_objects()\n"
"                    % set request = sponsorship.env['sms.child.request'].search([('sponsorship_id', '=', sponsorship.id)])\n"
"                    % set child = sponsorship.child_id\n"
"% set image_url = child.image_url.replace('media.ci.org/', 'media.ci.org/g_face,c_thumb,w_150,h_150,r_max/') if child.image_url else None\n"
"                </div>\n"
"% if image_url:\n"
"<div style=\"text-align: center;&gt;\n"
"&lt;img src=\">\n"
"</div>\n"
"% endif\n"
"<h1 style=\"text-align: center; line-height: 1.2;\">Thank you for your sponsorship!</h1>\n"
"                <p>\n"
"                    You chose to invest in the life of ${child.preferred_name}. There are two more steps before ${child.get('he')} will be informed that you sponsor ${child.get('him')}, but you don't have to hurry. We have reserved the child so that you can take your time to complete the registration.\n"
"                    <br><br>\n"
"                    1. You can <a href=\"${request.full_url}\" style=\"color: #0055A6; text-decoration: underline;\">click here</a> to complete your information and your payment option in the coming days.\n"
"                    <br>\n"
"                    2. Your sponsorship will be activated when we receive your first payment.\n"
"                    <br><br>\n"
"                    Best regards,\n"
"<br><br>\n"
" ${object.user_id.preferred_name} ${object.user_id.lastname}\n"
"   <br>\n"
"   ${object.user_id.employee_ids.department_id.name}\n"
"   <br>\n"
"   ${object.user_id.company_id.address_name}\n"
"                </p>\n"
"            "
msgstr "<div>\n"
"   % set sponsorship = object.get_objects()\n"
"                    \n"
"% set request = sponsorship.env['sms.child.request'].search([('sponsorship_id', '=', sponsorship.id)])\n"
"                    \n"
"% set child = sponsorship.child_id\n"
"                    \n"
"% set image_url = child.image_url.replace('media.ci.org/', 'media.ci.org/g_face,c_thumb,w_150,h_150,r_max/') if child.image_url else None\n"
"  </div>\n"
"% if image_url:  \n"
"<div style=\"text-align: center;\">\n"
"   <img src=\"${image_url}\">\n"
"  </div>\n"
"% endif\n"
"  <h1 style=\"text-align: center; line-height: 1.2;\">\n"
"   Grazie per il tuo sostegno!\n"
"  </h1>\n"
"  <p>\n"
"   Ciao!\n"
"   <br>\n"
"  </p>\n"
"  <p>\n"
"   Hai deciso in investire nella vita di ${child.preferred_name}.\n"
"  </p>\n"
"  <p>\n"
"   Ci sono solo due piccoli passi prima che ${child.get('lui')} venga informato che hai deciso di ${'sostenerlo' if child.gender == 'M' else 'sostenerla'}.\n"
"   <br>\n"
"   <br>\n"
"   1. <a href=\"${request.full_url}\" style=\"color: #0055A6; text-decoration: underline;\">Clicca qui</a> per completare i tuoi dati, indirizzo etc e scegliere un metodo di pagamento.\n"
"   <br>\n"
"   2. Una volta arrivato il tuo primo versamento il bambino verrà informato che sei diventato il suo sostenitore. Insieme a te, liberiamo i bambini dalla povertà estrema, un bambino alla volta.\n"
"   <br>\n"
"   <br>\n"
"  </p>\n"
"  <p>\n"
"   Un sostegno cambia la vita di un bambino in modo duraturo. Michelle, cresciuta in estrema povertà nelle Filippine, per esempio, da bambina è entrata nel programma di sostegno a distanza ed ha trovato un sostenitore. Oggi, adulta, dirige la propria organizzazione di aiuto per le donne che sono state vittime della tratta di esseri umani.\n"
"  </p>\n"
"  <p>\n"
"   La vita di ${child.preferred_name} cambierà per sempre grazie al tuo gesto. Grazie per viaggiare al nostro fianco in questa missione.\n"
"  </p>\n"
"  <p>\n"
"   Cordiali saluti,\n"
"  </p>\n"
"  <p>\n"
"<br>\n"
"    ${object.user_id.short_signature | safe}\n"
"  </p>\n"
" "

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_sponsorship_completion
msgid "<div>\n"
"                    % set sponsorships = object.get_objects()\n"
"                    % set child = sponsorships.mapped('child_id')\n"
"                    % set firstname = child.get_list('preferred_name', 3, child.get_number(), False)\n"
"                    % set him = child.get('him')\n"
"                    % set month = child[0].birthday_month\n"
"    % set correspondent = object.partner_id in sponsorships.mapped('correspondent_id')\n"
"                </div>\n"
"                <p>\n"
"                    ${object.partner_id.salutation},\n"
"                    <br>\n"
"                    <br>\n"
"                    You have been sponsoring ${firstname + ' '}\n"
"                    % if len(child) == 1:\n"
"                        from ${child.project_id.country_id.name + ' '}\n"
"                    % endif\n"
"                    for many years. We would like to thank you once more for your generosity and help to the children in need.\n"
"                    <br>\n"
"                    <br>\n"
"                    <b>We would like to inform you, that ${child.get('your sponsored child')} will complete the Compassion program and end the activities in the project in ${child.get_completion()}.</b>\n"
"                    <br>\n"
"                    <br>\n"
"% if correspondent:\n"
"                    We would warmly advice you to send an encouraging letter before the end of the program and if you want, send ${him} a gift.\n"
"                    <br>\n"
"                    <br>\n"
"% endif\n"
"                    For any further questions or remarks, please keep in touch with us.\n"
"                    <br>\n"
"                    <br>\n"
"                    Yours sincereley,\n"
"                </p>\n"
"            "
msgstr ""

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_wrpr_dossier
msgid "<div>\n"
"    % set sponsorships = object.get_objects()\n"
"    % set child = sponsorships.mapped('child_id')\n"
"    % set firstname = child.get_list('preferred_name', 3, child.get_number(), False)\n"
"    % set partner = object.partner_id\n"
"    % set payer = object.partner_id in sponsorships.mapped('partner_id') and len(sponsorships.filtered('period_paid')) != len(sponsorships)\n"
"</div>\n"
"<p>\n"
"    ${partner.informal_salutation},\n"
"    <br>\n"
"    <br>\n"
"    Welcome to the Compassion Family!\n"
"    <br>\n"
"    <br>\n"
"    Thank you for your commitment to ${'another ' if sponsorships.mapped('parent_id') or len(object.partner_id.sponsorship_ids) != len(sponsorships) else ''}child in poverty. You have made a great decision! You will be changing the life of ${firstname} ${child.get('his')} family.\n"
"    <br>\n"
"    <br>\n"
"    In a few days time, ${firstname} will learn that ${child.get('he')} ${child.get('has')} a sponsor. It will be a day of joy for ${child.get('him')} and ${child.get('his')} family.\n"
"    <br>\n"
"    <br>\n"
"    So that the sponsorship runs smoothly, enclosed you will find some important information and documents to help you get started:\n"
"    <br>\n"
"</p>\n"
"<ul>\n"
"    <li>\n"
"        1 photo of your sponsored child\n"
"    </li>\n"
"    <li>\n"
"        1 dossier with practical information about this sponsorship\n"
"        <br>\n"
"    </li>\n"
"    <li>\n"
"        1 letter template so that you can introduce yourself to the child\n"
"        <br>\n"
"    </li>\n"
"    <li>\n"
"        1 sheet of labels to use each time you write letters to the child (1 label each with a reference number to whatever you send-- each letter, card, drawing, booklet, etc.)\n"
"        <br>\n"
"    </li>\n"
"</ul>\n"
"<p>\n"
"    The special feature of a “Write &amp; Pray” sponsorship is teamwork. The sponsored child will come to know you as ${child.get('his')} sponsor.  We are depending on you to pray for the child and to write to ${child.get('him')} at least 2x per year while another donor provides the ${'missing ' if payer else ''}financial support for ${child.get('the child')}.\n"
"    <br>\n"
"    <br>\n"
"    We wish you and ${firstname} much joy and blessing in corresponding with one another!\n"
"    <br>\n"
"    <br>\n"
"    If you have any questions, please do not hesitate to contact us.\n"
"    <br>\n"
"    <br>\n"
"    Best regards,\n"
"</p>\n"
" "
msgstr "<div>\n"
"   % set sponsorships = object.get_objects()\n"
"    \n"
"% set child = sponsorships.mapped('child_id')\n"
"    \n"
"% set firstname = child.get_list('preferred_name', 3, child.get_number(), False)\n"
"    \n"
"% set partner = object.partner_id\n"
"\n"
"% set cher = partner.get('salutation').title() if not partner.title.plural else 'Cari'\n"
"\n"
"% set young = True\n"
"     \n"
"% if partner.title.plural:\n"
"	\n"
"% set young = False\n"
"\n"
"% endif \n"
"        \n"
"% set il_vostro = 'il tuo' if young else 'il vostro'\n"
"\n"
"% set lui = child.get('lui')\n"
"\n"
"% set payer = partner in sponsorships.mapped('partner_id') and len(sponsorships.filtered('period_paid')) != len(sponsorships)\n"
"  </div>\n"
"  <p>\n"
"   ${partner.informal_salutation}\n"
"   <br>\n"
"   <br>\n"
"   % if sponsorships.mapped('parent_id') or len(object.partner_id.sponsorship_ids) != len(sponsorships):\n"
"	Grazie per ${il_vostro} ulteriore impegno in favore dei bambini bisognosi.\n"
"	% else:\n"
"	${partner.get('Benvenuto')} nella famiglia dei sostenitori di Compassion!\n"
"% endif \n"
"   ${'Hai' if young else 'Avete'} appena preso una decisione generosa. Una decisione che cambierà in modo duraturo la vita di ${firstname} e delle persone a ${lui} vicino.\n"
"   <br>\n"
"   <br>\n"
"   Tra qualche giorno ${firstname} saprà che cè qualcuno che ha deciso di ${child.get('sostenerlo')}! Sarà un giorno di festa per ${lui} per la sua famiglia e le persone vicino.\n"
"   <br>\n"
"   <br>\n"
"   Affinchè ${il_vostro} sostegno possa iniziare nel modo migliore, ${'troverai' if young else 'troverete'} in allegato informazioni e documenti importanti:\n"
"  </p>\n"
"  <ul>\n"
"   <li>\n"
"    1 foto ${'del bambino' if len(child) == 1 else 'dei bambini'}.\n"
"    <br>\n"
"   </li>\n"
"   <li>\n"
"    1 dossier con informazioni pratiche sul sostegno a distanza\n"
"    <br>\n"
"   </li>\n"
"   <li>\n"
"    1 cartolina di presentazione da inviare ${'al bambino' if len(child) == 1 else 'ai bambini'}\n"
"    <br>\n"
"   </li>\n"
"   <li>\n"
"    1 foglio di etichette da utilizzare per tutta la corrispondenza con ${'il bambino' if len(child) == 1 else 'i bambini'} (da incollare su lettere, carte, disegni, adesivi….). Qui sono indicati i numeri di riferimento del sostenitore e i numeri di riferimento ${'del bambino' if len(child) == 1 else 'dei bambini'}\n"
"    <br>\n"
"   </li>\n"
"  </ul>\n"
"  La particolarità del sostegno «Write & Pray» è il lavoro di squadra. ${'Tu sarai il sostenitore' if young else 'Voi sarete i sostenitori'} in contatto con ${firstname}. Contiamo su ${'di te' if young else 'di voi'} per pregare per ${lui}, per scrivere entro il primo mese una letterina e assicurarne almeno due al bambino durante l'anno. Un altro donatore fornirà supporto finanziario per questo sostegno.\n"
"  <br>\n"
"  <br>\n"
"  % if payer:\n"
"	Un altro donatore si farà carico della rimanenza del sostegno finanziario. ${il_vostro} ${child.get('child')} ${child.get('will be')} informat${child.get('accord_o_a')} di avere un sostenitore, dopo la prima donazione che aspettiamo nei prossimi giorni.\n"
"	% else:\n"
"	Un altro donatore fornirà supporto finanziario per questo sostegno.\n"
"% endif\n"
"  <br>\n"
"  <br>\n"
"  ${firstname} sarà una fonte di gioia e di benedizione per entrambi! ${'Non esitare' if young else 'Non esitate'} a prendere contatto con noi per eventuali domande.\n"
"  <br>\n"
"  <br>\n"
"  ${'Ti' if young else 'Vi'} preghiamo di recevere i nostri migliori saluti.\n"
" "

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_child_transfer
msgid "<div>\n"
"   % set child = object.get_objects()\n"
"    \n"
"% set lifecycle = child.lifecycle_ids[0]\n"
"    \n"
"% set name = child.preferred_name or child.firstname\n"
"     \n"
"% set icp = child.project_id\n"
"  </div>\n"
"  <p>\n"
"   ${object.partner_id.salutations}\n"
"   <br>\n"
"   <br>\n"
"   Thank you for investing in the life of ${child.firstname} with a sponsorship. ${child.get('his').title()} life is being changed permanently.\n"
"   <br>\n"
"   <br>\n"
"   We have received a message that your sponsored child has moved. Fortunately, there is also a church at their new place of residence that works together with Compassion. ${child.firstname} was registered at the Child Development Center of the local church. Therefore, ${child.get('he')} has been issued a new reference number, ${child.local_id}.\n"
"   <br>\n"
"   <br>\n"
"   The new Child Development Center of ${child.firstname} is close to ${icp.closest_city}. There are ${icp.nb_cdsp_kids} children registered. Enclosed, you will find a new sheet of labels for your letters which is valid from today. If you have any questions about your sponsorship, please do not hesitate to contact us.\n"
"   <br>\n"
"   <br>\n"
"   Over 65 years of experience shows us that sponsorships release children from poverty efficiently and sustainably. ${object.success_sentence or ''}\n"
"   <br>\n"
"   <br>\n"
"   We wish you much joy and blessings in corresponding with your sponsored child.\n"
"   <br>\n"
"   <br>\n"
"   Kind regards,\n"
"  </p>\n"
" "
msgstr "<div>\n"
"   % set child = object.get_objects()\n"
"   \n"
"% set name = child.get_list('preferred_name')\n"
"   \n"
"% set fcp = child.mapped('project_id')\n"
"    \n"
"% set lifecycle = child.lifecycle_ids[:1]\n"
"    \n"
"% set one = len(child) == 1\n"
"  </div>\n"
"  <p>\n"
"   ${object.partner_id.salutation},\n"
"   <br>\n"
"   <br>\n"
"   Grazie di cuore per investire nella vita di ${name} attraverso il sostegno a distanza. Sta cambiando la ${child.get('sua')} vita in modo duraturo.\n"
"   <br>\n"
"   <br>\n"
"   Abbiamo ricevuto la notizia che ${child.get('il suo bambino')} ${child.get('is')} ${child.get('stato trasferito')} in un altro centro Compassion, causa trasferimento. Di conseguenza,\n"
"% if one:\n"
"   ha un nuovo numero di riferimento: ${child.local_id}.\n"
"   % else:\n"
"	hanno dei nuovi numeri di riferimento:\n"
"  </p>\n"
"  <ul>\n"
"   % for _child in child:\n"
"   <li>\n"
"    ${_child.preferred_name} : ${_child.local_id}\n"
"   </li>\n"
"   % endfor\n"
"  </ul>\n"
"  <p>\n"
"   % endif \n"
"   <br>\n"
"   <br>\n"
"   In allegato le inviamo delle nuove etichette da utilizzare d’ora in poi per la corrispondenza. Per qualsiasi domanda relativa al sostegno non esiti a contattarci.\n"
"  <br>\n"
"  <br>\n"
"  Oltre 60 anni di esperienza nel sostegno a distanza ci hanno dimostrato che il programma è efficace e libera i bambini dalla povertà. ${object.success_sentence or ''}\n"
"  <br>\n"
"  <br>\n"
"  Ancora una volta: grazie del suo impegno al nostro fianco, che permette ad un bambino di essere liberato dalla povertà estrema per vivere una vita dignitosa e con uno scopo.\n"
"  <br>\n"
"  <br>\n"
"  Le auguriamo gioia e benedizioni continue nella corrispondenza con ${name}.\n"
"  <br>\n"
"  <br>\n"
"  Con i nostri piú calorosi saluti.\n"
"  <br>\n"
"  <br>\n"
"  P.S. Ha già l'App di Compassion? Caricare le foto, le lettere e tutte le informazioni del suo bambino sul suo cellulare. Scrivere non è mai stato così facile! Per scaricare la App: https://compassion.ch/it/app\n"
"</p>\n"
" "

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_child_reinstatement
msgid "<div>\n"
"   % set child = object.get_objects()\n"
"    \n"
"% set partner = object.partner_id\n"
"  </div>\n"
"  <p>\n"
"   ${partner.salutation}\n"
"   <br>\n"
"   <br>\n"
"   We have received good news! ${child.get('your sponsored child')}${child.get_list('preferred_name')} has returned to the project of the local church and has been re-enrolled in the sponsorship program, because ${child.lifecycle_ids[:1].translate('request_reason')}.\n"
"   <br>\n"
"   <br>\n"
"   You were previously the sponsor of ${child.get_list('preferred_name')}. Thus we would like to offer you the opportunity to sponsor ${child.get_list('preferred_name')} again and continue to invest in ${child.get('his')} life in order to release ${child.get('him')} from poverty in the long-term. We thank you in advance for a quick answer.\n"
"   <br>\n"
"   <br>\n"
"   After more than 60 years of experience, we at Compassion believe: Taking on a sponsorship changes the life of a child, their family and their surroundings. ${object.success_sentence or ''}\n"
"   <br>\n"
"   <br>\n"
"   Once again, thank you for your commitment so that together with you and our 8000 partner churches in the global south, we are able to transform the lives of more than 2 million children in 25 countries.\n"
"   <br>\n"
"   <br>\n"
"   Kind Regards,\n"
"% if object.send_mode == 'digital':\n"
"   <br>\n"
"   <br>\n"
"    ${object.user_id.signature | safe}\n"
"\n"
"% endif\n"
"  </p>\n"
" "
msgstr ""

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_parent_alive
msgid "<div>\n"
"   % set child = object.get_objects()\n"
"                    \n"
"% set role = 'mother' if child.household_id.revised_value_ids.name == 'Natural Mother Alive' else 'father'\n"
"                    \n"
"% set status = 'alive' if child.household_id.revised_value_ids.get_field_value() == 'True' else 'dead'\n"
"  </div>\n"
"  <p>\n"
"   ${object.partner_id.salutation}\n"
"   <br>\n"
"   <br>\n"
"   Thank you for investing in the life of ${child.firstname} with a sponsorship.\n"
"   <br>\n"
"   <br>\n"
"   We have been notified from our local employees that the ${role} of your sponsored child has passed away. If you would like to write a few lines expressing your condolences to comfort ${child.get('him')}, this can be easily done online:  https://compassion.ch/de/schreiben/.\n"
"   <br>\n"
"   <br>\n"
"   In this difficult time, your support for ${child.firstname} makes an even bigger difference.\n"
"   <br>\n"
"   <br>\n"
"   Once again, thank you for your commitment so that together we can positively transform the lives of currently more than 2 million children in 25 countries.\n"
"   <br>\n"
"   With kind regards,\n"
"% if object.send_mode == 'digital':\n"
"   <br>\n"
"   <br>\n"
"     ${object.user_id.signature | safe}\n"
"% endif\n"
"  </p>\n"
" "
msgstr ""

#. module: partner_communication_switzerland
#: model:mail.template,body_html:partner_communication_switzerland.email_illness
msgid "<div>\n"
"   % set child = object.get_objects()\n"
"  </div>\n"
"  <p>\n"
"   ${object.partner_id.salutation},\n"
"   <br>\n"
"   <br>\n"
"\n"
"    Thank you for investing in the life of ${child.preferred_name} with a sponsorship.\n"
"   <br>\n"
"   <br>\n"
"    In Switzerland and also in the country where ${child.preferred_name}     lives, healthcare problems sometimes occurdespite medical care and support. Our staff at the Children‘s Center haveinformed us that ${child.preferred_name} has ${child.current_values}. It is a chronic illness.\n"
"   <br>\n"
"   <br>\n"
"    Thanks to your regular support and through a special fund for medical emergencies, Compassion is committed to the treatment and care of ${child.preferred_name}. If you accompany ${child.get('him')} in prayer now and ask for God‘s blessings andprotection, you are taking on an important service for this child!\n"
"      <br>\n"
"   <br>\n"
"    Your support of a child in extreme poverty is valuable. At Compassion, weare convinced that this will give a child the chance for a dignified and meaningfullife. ${object.success_sentence or ''}\n"
"     <br>\n"
"   <br>\n"
"    Thank you again for your committment, so that together we can positivelytransform the lives of currently more than 2 million children in 25countries.\n"
"     <br>\n"
"   <br>\n"
"    Best regards,\n"
"% if object.send_mode == 'digital':\n"
"   <br>\n"
"   <br>\n"
"      ${object.user_id.signature | safe}\n"
"% endif\n"
"  \n"
"  </p>\n"
" "
msgstr ""

