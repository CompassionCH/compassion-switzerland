<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_payment_transaction_form" model="ir.ui.view">
        <field name="name">payment.transaction.muskathlon.form</field>
        <field name="model">payment.transaction</field>
        <field name="inherit_id" ref="payment.transaction_form"/>
        <field name="arch" type="xml">
            <field name="reference" position="after">
                <field name="registration_id" attrs="{'invisible': [('registration_id','=',False)]}"/>
                <field name="invoice_id" attrs="{'invisible': [('invoice_id','=',False)]}"/>
            </field>
        </field>
    </record>
</odoo>
