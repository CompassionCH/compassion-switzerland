<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_muskathlon_registration_form" model="ir.ui.view">
        <field name="name">muskathlon.registration.form</field>
        <field name="model">muskathlon.registration</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="event_id"/>
                            <field name="partner_id"/>
                            <field name="muskathlon_participant_id"/>
                        </group>
                        <group>
                            <field name="muskathlon_event_id"/>
                            <field name="sport_type"/>
                            <field name="reg_id"/>
                        </group>
                    </group>
                    <group>
                        <field name="ambassador_description"/>
                        <field name="ambassador_quote"/>
                    </group>
                    <group>
                        <group>
                            <field name="ambassador_picture_1"/>
                            <field name="amount_objective"/>
                            <field name="amount_raised_percents"/>
                        </group>
                        <group>
                            <field name="ambassador_picture_2"/>
                            <field name="amount_raised"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
