#. module: muskathlon
#: model:mail.template,body_html:muskathlon.ambassador_donation_confirmation_template
msgid ""
"<div>\n"
"                    % set invoice_lines = object.get_objects()\n"
"                    % set donations = invoice_lines.get_donations()\n"
"                    % set partner = object.partner_id\n"
"                    % set donors = invoice_lines.mapped('partner_id')\n"
"                    % set muskathlon = partner.registration_ids[:1]\n"
"% set url = 'http://www.muskathlon-kilimanjaro.ch/en_US' + muskathlon."
"website_url\n"
"                </div>\n"
"                <div>\n"
"                    ${partner.salutation},\n"
"                    <br/>\n"
"                    <br/>\n"
"                    Compassion Switzerland is glad to inform you that you "
"just received a donation of CHF ${donations[0]}.-\n"
"                    % if donations[1]:\n"
"                        for the ${donations[1]}.\n"
"                    % endif\n"
"                    <br/>\n"
"                    <br/>\n"
"                    % if len(donors) == 1:\n"
"                        Your donor is ${donors.preferred_name} from ${donors."
"city}.\n"
"                        % if donors.email and donors.thankyou_letter != "
"'no':\n"
"                    You can personally thank ${donors.get('him')} if you "
"wish by sending ${donors.get('him')} an e-mail at <a href=\"mailto:${donors."
"email}\" style=\"color: skyblue; text-decoration: underline;\">${donors."
"email}</a>. In any case, ${donors.get('he')} will receive a thank you letter "
"from us.\n"
"                        % endif\n"
"                    % else:\n"
"                        You received this from the following people. You may "
"contact them by e-mail if you wish to thank them personally.\n"
"                        <ul>\n"
"                            % for donor in donors:\n"
"                            <li>${donor.preferred_name} from ${donor.city} "
"${'(<a href=\"{}\" style=\"color: skyblue; text-decoration: underline;\">{}</"
"a>)'.format(donor.email, donor.email) if donor.email and donor."
"thankyou_letter != 'no' else '' |safe}</li>\n"
"                            % endfor\n"
"                        </ul>\n"
"                    % endif\n"
"                    % if muskathlon:\n"
"                        You currently have raised CHF ${muskathlon."
"amount_raised}.- You can check your barometer at anytime\n"
"                        <a href=\"${url}\" style=\"color: skyblue; text-"
"decoration: underline;\">on your sport profile page</a>.\n"
"                    % endif\n"
"                    <br/>\n"
"                    <br/>\n"
"                    Yours sincerely\n"
"% if object.send_mode == 'digital':\n"
"   <br/>\n"
"   <br/>\n"
"    Rachel Maglo\n"
"    <br/>\n"
"    Donors relation\n"
"    <br/>\n"
"    Compassion Switzerland\n"
"% endif\n"
"                </div>\n"
"            "
