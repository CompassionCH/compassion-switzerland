<odoo>
    <data>
        <template id="crowdfunding_project_list_header" name="Header for project list page">
            <t t-call="theme_crowdfunding.crowdfunding_navbar"/>
            <header class="masthead">
                <div class="container">
                    <div class="intro-text">
                        <h1 class="display-1 blue">JOIN A GREAT PROJECT</h1>
                        <h2 class="display-4 pb-4 ">Two lines explanation or introduction about project launch</h2>
                        <br/>
                    </div>
                </div>
            </header>
        </template>

        <template id="crowdfunding_project_creation_header" name="Header for project creation page">
            <t t-call="theme_crowdfunding.crowdfunding_navbar"/>
            <header class="masthead">
                <div class="container">
                    <div class="intro-text">
                        <br/>
                        <t t-set="img_path" t-value="'/theme_crowdfunding/static/src/img/DSC-16.jpg'"/>
                        <t t-call="theme_crowdfunding.crowdfunding_rounded_image"/>
                        <br/>
                        <br/>
                        <h1 class="display-1 blue">CREATE A NEW PROJECT</h1>
                        <h2 class="display-4 pb-4 ">Two lines explanation or introduction about project launch</h2>
                        <br/>
                        <br/>
                        <t t-call="theme_crowdfunding.crowdfunding_progress_bar_icons"/>
                    </div>
                </div>
            </header>
        </template>
    </data>
</odoo>