<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="group_visit_demo" model="crm.event.compassion">
        <field name="name">Group visit in Kenya</field>
        <field name="event_type_id" ref="event_type_group_visit"/>
        <field name="type">tour</field>
        <field name="start_date" eval="(datetime.now() + timedelta(days=3)).strftime('%Y-%m-%d')"/>
        <field name="end_date" eval="(datetime.now() + timedelta(days=10)).strftime('%Y-%m-%d')"/>
        <field name="hold_start_date" eval="datetime.now().strftime('%Y-%m-%d')"/>
        <field name="number_allocate_children">0</field>
        <field name="planned_sponsorships">0</field>
    </record>
</odoo>
