<odoo>
    <record model="ir.ui.view" id="mis_spn_event_info_tree_view">
        <field name="name">mis.spn_event.info.tree </field>
        <field name="model">mis.spn_event.info</field>
        <field name="arch" type="xml">
            <tree>
                <field name="line_type"/>
                <field name="date"/>
                <field name="name"/>
                <field name="partner_id"/>
                <field name="user_id"/>
                <field name="account_id"/>
                <field name="debit" sum="debit" />
                <field name="credit" sum="credit" />
            </tree>
        </field>
    </record>
</odoo>
