<openerp>
    <data>
        <record model="ir.ui.view" id="field_office_form">
            <field name="name">field.office.form</field>
            <field name="model">compassion.field.office</field>
            <field name="inherit_id" ref="child_compassion.view_compassion_field_office_form"/>
            <field name="arch" type="xml">
                <field name="country_information" position="after">
                    <field name="fr_country_info_pdf" widget="binary" filename="fr_filename"/>
                    <field name="de_country_info_pdf" widget="binary" filename="de_filename"/>
                    <field name="it_country_info_pdf" widget="binary" filename="it_filename"/>
                    <field name="en_country_info_pdf" widget="binary" filename="en_filename"/>
                    <field name="fr_filename" invisible="1"/>
                    <field name="de_filename" invisible="1"/>
                    <field name="it_filename" invisible="1"/>
                    <field name="en_filename" invisible="1"/>
                </field>
            </field>
        </record>
    </data>
</openerp>
