<odoo>
    <record id="website_add_theme_view" model="ir.ui.view">
        <field name="name">website.theme.view</field>
        <field name="model">website</field>
        <field name="inherit_id" ref="website.view_website_form"/>
        <field name="arch" type="xml">
            <field name="domain" position="after">
                <field name="theme_id" type="col"/>
            </field>
        </field>
    </record>
</odoo>
