<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="crowdfunding_project_list" model="website.page">
            <field name="url">/project_list</field>
            <field name="website_indexed" eval="True"/>
            <field name="website_published">True</field>
            <field name="name">Project List</field>
            <field name="type">qweb</field>
            <field name="key">crowdfunding.project_list</field>
            <!--            TODO: change 12-->
            <!--            <field name="website_id">12</field>-->
            <field name="arch" type="xml">
                <t name="Project List" t-name="crowdfunding.project_list">
                    <t t-call="website.layout">
                        <t t-call="theme_crowdfunding.crowdfunding_navbar"/>

                        <t t-set="card"/>
                        <t t-call="theme_crowdfunding.crowdfunding_card_container"/>

                        <t t-call="theme_crowdfunding.crowdfunding_footer"/>
                    </t>
                </t>
            </field>
        </record>
    </data>
</odoo>




