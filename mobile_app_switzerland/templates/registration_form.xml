<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="mobile_registration_success" inherit_id="mobile_app_connector.mobile_registration_success">
        <xpath expr="//div[hasclass('col-md-12')]">
            <br/>
            <br/>
            <t t-if="source == 'myaccount'">
                <a href="/my/home" class="btn btn-primary">Back to Login</a>
            </t>
            <t t-else="">
                <a t-att-href="app_url" class="btn btn-primary">Back to the App</a>
            </t>
        </xpath>
    </template>
</odoo>
