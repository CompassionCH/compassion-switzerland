<odoo>
    <!-- Configuration View -->
    <record id="view_notification_settings_website_form" model="ir.ui.view">
        <field name="name">staff.notification.website.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="child_compassion.compassion_settings"/>
        <field name="arch" type="xml">
            <field name="disaster_notify_ids" position="after">
                <field name="sponsorship_fr_id"/>
                <field name="sponsorship_de_id"/>
                <field name="sponsorship_it_id"/>
            </field>
        </field>
    </record>
</odoo>
