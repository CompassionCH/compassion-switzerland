            <field name="name">My Sponsorships</field>
            <field name="url">/my/children</field>
            <field name="parent_id" ref="website.main_menu" />
        </record>
        <record id="my_children" model="website.menu">
            <field name="name">My personal data</field>
            <field name="url">/my/information</field>
