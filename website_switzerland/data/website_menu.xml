        </record>
        <record id="my_donations" model="website.menu">
            <field name="name">My Donations</field>
            <field name="url">/my/donations</field>
            <field name="parent_id" ref="website.main_menu" />
        </record>
        <record id="my_letters" model="website.menu">
            <field name="name">Write a letter</field>
            <field name="url">/my/letter</field>
            <field name="parent_id" ref="website.main_menu" />
        </record>
        <record id="faq" model="website.menu">
            <field name="name">Frequently asked questions</field>
            <field name="url">https://compassion.ch/questions-frequentes</field>
