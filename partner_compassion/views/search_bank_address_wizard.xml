<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="search_bank_address_wizard_form" model="ir.ui.view">
        <field name="name">search.bank.address.wizard.form</field>
        <field name="model">search.bank.address.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <div class="oe_title">
                    <h3
          >Existing Partner info in bank data (May be unavailable)</h3>
                </div>
                <group>
                    <field name="move_id" invisible="True" />
                    <field name="partner_address" />
                    <field name="date" />
                </group>
                <group>
                    <field name="overwriting_street" />
                    <field name="overwriting_zip_id" />
                </group>
                <footer>
                    <button
            name="change_address"
            type="object"
            string="Change address"
            class="btn-primary"
          />
                    <button
            name="cancel_change"
            type="object"
            string="Cancel change"
            class="btn-default"
          />
                </footer>
            </form>
        </field>
    </record>
</odoo>
